{
	"DATASET_SETTING": {
		"DATASET_NAME": "PascalVOC",
		"exclude_willow_classes": false,
		"train_sampling": "intersection",
		"eval_sampling": "intersection"
	},
	"TRAIN": {
		"MODULE": "models.PI_SK.model",
		"LOSS_FUNC": "models.HammingLoss",
		"BATCH_SIZE": 8,
		"EPOCH_ITER": 2000,
		"LR": 0.0002,
		"lr_schedule": {
			"num_epochs": 10,
			"lr_milestones": [2, 4, 6, 8, 10],
			"lr_decay": 0.5
		}
	},
	"EVAL": {
		"SAMPLES": 1000
	},
	"SOLVER_SETTING": {
		"PI_iter_nums": 25,
		"PI_stop_thresh":1.0e-3,
		"SK_iter_nums": 20,
		"SK_epsilon": 1.0e-10
	},
	"": {
		"save_checkpoint": false,
		"warmstart_path": null,
		"STATISTIC_STEP": 20,
		"visualize": false,
		"visualization_params": {
			"reduced_vis": true
		},
		"evaluate_only": false,
		"result_dir": "results/PI_SK/voc_basic"
	}
}