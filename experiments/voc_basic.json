{
  "BATCH_SIZE": 8,
  "DATASET_NAME": "PascalVOC",
  "exclude_willow_classes": false,
  "EVAL": {
    "SAMPLES": 100,
    "num_graphs_in_matching_instance": 2
  },
  "MIP": {
    "lambda_val": 80.0,
    "solver_params": {
      "LogToConsole" : 0,
      "NumericFocus" : 2,
      "Presovle" : 2,
      "LogFile" : "results/voc_basic/miplog.log"
    }
  },
  "train_sampling": "intersection",
  "eval_sampling": "intersection",
  "save_checkpoint": true,
  "RANDOM_SEED": 123,
  "warmstart_path": null,
  "TRAIN": {
    "EPOCH_ITERS": 200,
    "LR": 0.002,
    "lr_schedule": "long_halving",
    "num_graphs_in_matching_instance": 2
  },
  "STATISTIC_STEP": 20,
  "visualize": false,
  "visualization_params": {
    "reduced_vis": true
  },
  "evaluate_only": false,
  "model_dir": "results/voc_basic"
}
